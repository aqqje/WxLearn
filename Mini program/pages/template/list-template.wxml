<!--pages/template/list-template.wxml-->
<template name="listTem">
  <view class="temContainer">
    <view class="avatar_data">
      <image src="{{avatar}}"></image>
      <text>{{date}}</text>
    </view>
    <text class="company">{{author}}</text>
    <image class="contentImg" src="{{detail_img}}"></image>
    <text class="content">{{detail_content}}</text>
    <view class="collection_love">
      <image src="{{detail_love_image1}}"></image>
      <text>{{love_count}}</text>
      <image src="{{detail_love_image2}}"></image>
      <text>{{attention_count}}</text>
    </view>
  </view>
</template>
